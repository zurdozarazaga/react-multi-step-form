{
  "extends": [
    "react-app"
  ],
  "plugins": [
    "prettier",
    "react-hooks"
  ],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": false
      }
    ],
    "react-hooks/rules-of-hooks": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ]
  }
}